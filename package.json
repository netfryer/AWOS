{
  "name": "core",
  "version": "1.0.0",
  "description": "Task router for LLM model selection",
  "type": "module",
  "main": "dist/router.js",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "next": "^14.2.0",
    "openai": "^6.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^4.3.6"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "run": "npm run build && node dist/scripts/runTask.js",
    "run:ts": "tsx scripts/runTask.ts",
    "dev:ui": "npm run build && next dev -p 3000",
    "seed:model-hr": "tsx scripts/model-hr/seed.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "model-hr:sync": "tsx scripts/model-hr/syncProviders.ts",
    "model-hr:canary": "tsx scripts/model-hr/canary.ts",
    "model-hr:cycle": "tsx scripts/model-hr/cycle.ts"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  }
}